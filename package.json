{
  "name": "ensbook",
  "displayName": "ENSBook",
  "version": "3.0.2",
  "description": "Your private, efficient, and intelligent ENS portfolio manager. Local-first architecture.",
  "type": "module",
  "private": true,
  "author": {
    "name": "liubenben.eth",
    "url": "https://x.com/forlbb"
  },
  "license": "MIT",
  "homepage": "https://ensbook.xyz",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hibbb/ensbook.git"
  },
  "bugs": {
    "url": "https://github.com/hibbb/ensbook/issues"
  },
  "keywords": [
    "ens",
    "ethereum",
    "web3",
    "local-first",
    "react",
    "domain-manager"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "wagmi:generate": "wagmi generate",
    "tw-init": "tailwindcss init -p",
    "changelog": "git cliff --latest -o CHANGELOG.md",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "^4.0.10",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
